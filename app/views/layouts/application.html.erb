<!DOCTYPE html>
<html>
  <head>
    <title>MyNewHealthCare</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

			<nav class="navbar navbar-default navbar-fixed-top" id="nav">
			    <div class="container">
			        <!-- Brand and toggle get grouped for better mobile display -->
			        <div class="navbar-header">
			            <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
			                <span class="sr-only">Toggle navigation</span>
			                <span class="icon-bar"></span>
			                <span class="icon-bar"></span>
			                <span class="icon-bar"></span>
			            </button>
			        <%= link_to root_path do %>   
			    		<%= image_tag "SeeThrulogo2.png", class: "logo-pic" %>
			    		<% end %>
			        </div>
			        <!-- Collection of nav links and other content for toggling -->
			        <div id="navbarCollapse" class="collapse navbar-collapse">
			            <ul class="nav navbar-nav navbar-right" id="pages">
			            	<li><%= link_to "Home", root_path %></li>
						        <li><%= link_to "About Us", about_us_path %></li>
						        <li><%= link_to "Solution", our_solution_path %></li>
						        <li><%= link_to "Contact Us", contact_us_path %></li>
			            </ul>
			        </div>
			    </div><!-- Container End -->
			</nav>










			

    <%= yield %>
  </body>
</html>
